{
    "Kestrel": {
        "Endpoints": {
            "Https": {
                "Url": "https://localhost:5001"
            }
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "RandomConfig": {
        "DefaultSize": 32,
        "DefaultLimit": 1024,
        "DefaultWindow": 10,
        "LimitOverrides": {
            "1": {
                "Limit": 512,
                "Window": 5
            },
            "2": {
                "Limit": 768
            },
            "4": {
                "Limit": 1536,
                "Window": 10
            }
        }
    }
}
